{
  "profiles": {
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DISCOUNT_DB_CONNECTION_STRING": "mongodb://root:root12345@localhost:27017/",
